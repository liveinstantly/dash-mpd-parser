{
  "name": "dash-mpd",
  "version": "0.1.0",
  "description": "A MPEG-DASH MPD manifest parser.",
  "keywords": [
    "MPEG-DASH",
    "DASH",
    "dash",
    "MPD",
    "mpd",
    "manifest",
    "parser",
    "video",
    "adaptive",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liveinstantly/dash-mpd-parser"
  },
  "author": "Shige Fukushima <shigeyf@liveinstantly.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/liveinstantly/dash-mpd-parser/issues"
  },
  "homepage": "https://github.com/liveinstantly/dash-mpd-parser#readme",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "compile": "tsc",
    "build": "npm-run-all prepare clean compile",
    "lint": "eslint",

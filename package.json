  "devDependencies": {
    "@types/jest": "27.4",
    "@types/node": "16",
    "@types/node-fetch": "2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "jest": "27.4",
    "node-fetch": "2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
